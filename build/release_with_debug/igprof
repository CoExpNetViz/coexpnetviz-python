#!/bin/sh
set -e

finish() {
    trap - 2
    igprof-analyse -d -v -g igprof.pp.gz >& igprof.pp.report.res
    igprof-analyse --sqlite -d -v -g igprof.pp.gz | sqlite3 igprof.pp.report.sql3
}

. ../$1_command.sh

rm -f igprof.pp.gz
trap finish 2
igprof -d -pp -z -o igprof.pp.gz  ${command[@]} >& igprof.pp.log
finish
